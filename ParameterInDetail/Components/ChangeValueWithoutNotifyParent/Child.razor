<div class="bg-success p-5">
    <h3>Child</h3>
    <div>Value: @Value</div>
    <button @onclick="ChangeRandomValue" type="button">Change random value</button>
</div>

@code {
    [Parameter]
    public int Value { get; set; }

    public void ChangeRandomValue()
    {
        var random = new Random();
        Value = random.Next(0, 100);
    }
}
